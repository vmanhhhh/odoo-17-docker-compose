<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="dh_categories_work_field_action" model="ir.actions.act_window">
        <field name="name">Mảng công tác</field>
        <field name="res_model">dh.categories.work_field</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p>Vui lòng tạo dữ liệu</p>
        </field>
    </record>

<record id="dh_categories_work_field_view_tree" model="ir.ui.view">
    <field name="name">dh_categories_work_field.view.tree</field>
    <field name="model">dh.categories.work_field</field>
    <field name="arch" type="xml">
        <tree string="">
            <field name="name" />
            <field name="write_date" string="Lần cập nhật cuối" />
        </tree>
    </field>
</record>

</odoo>